<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.edu.scut.sse.supply.upstreamenterprise.entity.pojo.UpstreamEnterpriseContract" table="upstream_enterprise_contract"
           schema="supply_chain">
        <id name="fid">
            <column name="fid" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="hash">
            <column name="hash" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="sponsor">
            <column name="sponsor" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="receiver">
            <column name="receiver" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="startDate">
            <column name="start_date" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>