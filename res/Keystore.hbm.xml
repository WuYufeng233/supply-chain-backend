<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.edu.scut.sse.supply.general.entity.pojo.Keystore" table="keystore" schema="supply_chain">
        <id name="code">
            <column name="code" sql-type="int(11)"/>
        </id>
        <property name="publicKey">
            <column name="public_key" sql-type="varchar(800)" length="800" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>