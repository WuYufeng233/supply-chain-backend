<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.edu.scut.sse.supply.coreenterprise.entity.pojo.CoreEnterpriseCargoReceive"
           table="core_enterprise_cargo_receive" schema="supply_chain">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="content">
            <column name="content" sql-type="varchar(1024)" length="1024" not-null="true"/>
        </property>
        <property name="consignor">
            <column name="consignor" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="time">
            <column name="time" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>